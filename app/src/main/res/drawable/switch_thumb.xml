<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="2dp"
        android:right="1dp"
        android:top="2.3dp"
        android:bottom="2.3dp">

        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">

            <size
                android:width="17dp"
                android:height="17dp" />

            <solid
                android:color="#FFFFFF" />


        </shape>
    </item>
</layer-list>

